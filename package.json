  "scripts": {
    "bootstrap": "./scripts/packages/bootstrap.sh",
    "build": "web-common-command-runner --command \"lerna run build\" --timeout \"300000\"",
    "build-sas": "./scripts/build-sas.sh",
    "build-sas-bundle": "ts-node ./scripts/build-sas-bundle.ts",
    "build-storybook": "web-common-command-runner --command \"build-storybook -c ./config/storybook --quiet\" --timeout \"180000\"",
    "clean": "lerna run clean --parallel && rm -f ./config/eslint/.eslintcache",
    "commit": "git commit",
    "cypress:log-error": "CYPRESS_LOG='error' ./scripts/packages/cypress-test.sh run --browser chrome",
    "cypress:log-trace": "CYPRESS_LOG='trace' ./scripts/packages/cypress-test.sh run --browser chrome",
    "cypress:open": "./scripts/packages/cypress-test.sh open",
    "cypress:run": "./scripts/packages/cypress-test.sh run",
    "cypress:run-chrome": "./scripts/packages/cypress-test.sh run --browser chrome",
    "cypress:run-firefox": "./scripts/packages/cypress-test.sh run --browser firefox",
    "dev-server": "./scripts/packages/dev.sh",
    "format": "prettier --ignore-path ./.gitignore --write",
    "lint": "./scripts/packages/lint.sh && node scripts/ci/check-mismatched-dependencies.js",
    "make": "cross-env TS_NODE_PROJECT='templates/tsconfig.json' plop --plopfile='templates/plopfile.ts' --dest='.' && lerna bootstrap",
    "nuke": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "patch-sas": "scripts/patch-sas.sh",
    "prepare": "husky install",
    "publish-sas": "yarn build-sas && ts-node ./scripts/publish-sas.ts",
    "publish-sas-integration": "ts-node ./scripts/publish-sas-integration.ts",
    "start": "yarn storybook",
    "start:fm": "./scripts/start-fm.sh",
    "storybook": "start-storybook -p 9754 -c ./config/storybook",
    "test": "./scripts/packages/test.sh",
    "upload-storybook": "node ./scripts/uploadDriveComponentsToS3.js",
    "upload-strings": "web-common-string-uploader --path=./packages/assets/drive-assets-strings/src/en_US --webId=AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAaA"
  },
  "dependencies": {
    "@workday/web-uic-mock": "^5.6.1",
    "eslint-config-prettier": "^7.0.0"
  },
  "resolutions": {
    "@types/react": "16.14.26"
  },
  "workspaces": [
    "packages/assets/*",
    "packages/components/*",
    "packages/entrypoints/*",
    "packages/hooks/*",
    "packages/cypress-test",
    "packages/io/*",
    "packages/processes/*",
    "packages/types/*"
  ],
  "prettier": "@workday/drive-asset-prettier",
  "config": {
    "commitizen": {
      "path": "./packages/assets/drive-assets-commitlint"
    }
  },
  "eslintConfig": {
    "extends": "./config/eslint/eslintrc.js"
  }
}
